<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#d2fcfa"
    tools:context=".Home">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/line"
            android:layout_width="200dp"
            android:layout_height="56dp"
            android:scaleType="fitXY"
            android:layout_marginEnd="215dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="8dp"
            android:src="@mipmap/line" />

        <ImageView
            android:id="@+id/self_awarness"
            android:layout_width="155dp"
            android:layout_height="100dp"
            android:scaleType="fitXY"
            android:src="@mipmap/self_awarness_icon"
            android:layout_marginStart="200dp"
            app:layout_constraintBottom_toTopOf="@+id/line"
            app:layout_constraintEnd_toEndOf="@+id/line"
            app:layout_constraintStart_toStartOf="@+id/line" />

        <ImageView
            android:id="@+id/clouds"
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:scaleType="fitXY"
            android:src="@mipmap/clouds"
            app:layout_constraintBottom_toTopOf="@+id/line"
            app:layout_constraintEnd_toStartOf="@id/self_awarness"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@id/self_awarness"
            android:layout_marginTop="50dp"/>

        <ImageView
            android:id="@+id/mind"
            android:layout_width="230dp"
            android:layout_height="100dp"
            android:layout_marginEnd="50dp"
            android:layout_marginBottom="80dp"
            android:scaleType="fitXY"
            android:src="@mipmap/mind_icon"
            app:layout_constraintBottom_toTopOf="@+id/clouds"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageView
            android:id="@+id/maountains"
            android:layout_width="280dp"
            android:layout_height="170dp"
            android:src="@mipmap/maountains"
            android:scaleType="fitXY"
            app:layout_constraintBottom_toTopOf="@+id/mind"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="60dp"/>

        <ImageView
            android:id="@+id/line3"
            android:layout_width="50dp"
            android:layout_height="150dp"
            android:src="@mipmap/line_3"
            app:layout_constraintBottom_toTopOf="@+id/clouds"
            app:layout_constraintEnd_toEndOf="@+id/mind"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginEnd="190dp"
            android:layout_marginBottom="100dp"
            />

        <ImageView
            android:id="@+id/line2"
            android:layout_width="40dp"
            android:layout_height="100dp"
            android:layout_marginStart="30dp"
            android:src="@mipmap/line_2"
            app:layout_constraintBottom_toTopOf="@+id/line3"
            app:layout_constraintEnd_toStartOf="@+id/maountains"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="200dp"/>

        <ImageView
            android:id="@+id/tree"
            android:layout_width="40dp"
            android:layout_height="100dp"
            android:layout_marginStart="20dp"
            android:src="@mipmap/tree"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/self_awarness"
            app:layout_constraintTop_toBottomOf="@+id/maountains" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="50dp"
            android:layout_height="70dp"
            android:layout_marginEnd="30dp"
            android:layout_marginBottom="180dp"
            android:scaleType="fitXY"
            android:src="@mipmap/line_5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/tree"
            app:layout_constraintStart_toEndOf="@+id/self_awarness"
            app:layout_constraintTop_toTopOf="@+id/self_awarness" />

        <ImageView
            android:id="@+id/lines"
            android:layout_width="45dp"
            android:layout_height="50dp"
            android:src="@mipmap/lines"
            app:layout_constraintBottom_toTopOf="@+id/tree"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/mind"
            app:layout_constraintTop_toBottomOf="@+id/maountains" />

        <ImageView
            android:layout_width="145dp"
            android:layout_height="110dp"
            android:layout_marginEnd="220dp"
            android:layout_marginTop="40dp"
            android:scaleType="fitXY"
            android:src="@mipmap/line_4"
            app:layout_constraintBottom_toBottomOf="@+id/imageView2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/mind"
            app:layout_constraintTop_toTopOf="@+id/mind"
            />

        <androidx.cardview.widget.CardView
            android:id="@+id/card_view"
            android:layout_width="150dp"
            android:layout_height="50dp"
            app:cardCornerRadius="25dp"
            app:cardElevation="8dp"
            android:layout_marginBottom="10dp"
            android:layout_marginEnd="20dp"
            app:layout_constraintBottom_toTopOf="@+id/self_awarness"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Mind-Mapping"
                android:textSize="15sp"
                android:textColor="#1f314a"
                android:layout_gravity="center"/>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="150dp"
            android:layout_height="50dp"
            app:cardElevation="8dp"
            app:cardCornerRadius="25dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/self_awarness"
            app:layout_constraintTop_toTopOf="@+id/self_awarness"
            android:layout_marginBottom="20dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#1f314a"
                android:text="Self-awareness"
                android:textSize="15sp"
                android:layout_gravity="center"/>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="165dp"
            android:layout_height="50dp"
            app:cardCornerRadius="25dp"
            app:cardElevation="8dp"
            app:layout_constraintBottom_toTopOf="@+id/lines"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/line2"
            app:layout_constraintTop_toTopOf="@+id/maountains"
            android:layout_marginTop="100dp"
            android:layout_marginStart="80dp"
            app:cardBackgroundColor="#ffffff">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Personal-Branding"
                android:layout_gravity="center"
                android:textSize="15sp"
                android:textColor="#1f314a"/>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="20dp"
            app:cardCornerRadius="25dp"
            app:cardElevation="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Yourself Journey"
                android:textSize="20sp"
                android:textStyle="bold"
                android:textColor="#1f314a"
                android:layout_gravity="center_horizontal"
                android:layout_marginEnd="30dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="end"
                android:layout_marginEnd="70dp"
                android:text="30%"
                android:textColor="#22d0c4"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Rank : 3rd"
                android:textColor="#1f314a"
                android:layout_gravity="end"
                android:textSize="20sp"
                android:textStyle="bold"
                android:layout_marginEnd="20dp"
                android:layout_marginTop="30dp"/>
            <de.hdodenhof.circleimageview.CircleImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:src="@drawable/man"
                android:layout_gravity="center_vertical"
                android:layout_marginStart="15dp"
                />
            <de.hdodenhof.circleimageview.CircleImageView
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:src="@mipmap/icon_4"
                android:layout_gravity="center"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="20dp"/>
            <de.hdodenhof.circleimageview.CircleImageView
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:src="@mipmap/icon_2"
                android:layout_gravity="center"
                android:layout_marginEnd="55dp"
                android:layout_marginTop="10dp"/>
            <de.hdodenhof.circleimageview.CircleImageView
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:src="@mipmap/icon"
                android:layout_gravity="center"
                android:layout_marginEnd="90dp"
                android:layout_marginTop="10dp"/>
        </androidx.cardview.widget.CardView>

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>